{
  "name": "react-boiler",
  "version": "0.0.1",
  "description": "A React / Webpack (w/ DevServer) / Jest (w/ Enzyme) Boilerplate application",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --port 8080 --config \"webpack.dev.config.js\"",
    "test": "node ./node_modules/jest/bin/jest.js",
    "update-snapshots": "node ./node_modules/jest/bin/jest.js --updateSnapshot",
    "lint-fix": "node ./node_modules/eslint/bin/eslint.js --fix \"./src/**/*.js\""
  },
  "author": "Ionut Ciucle",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "enzyme-to-json": "^3.2.1",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.9.0",
    "jest": "^21.2.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "es6-object-assign": "^1.1.0",
    "es6-promise": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
